<?xml version='1.0' encoding='utf-8'?>
<struct id="options::quiet_resampler_options" name="quiet_resampler_options" typedef="yes" xmlns="http://jessevdk.github.com/cldoc/1.0">
  <brief> Resampler options</brief>
  <doc>Controls arbitrary resample unit used by libquiet after generating 44.1kHz
signal or before decoding signal

This resampler will be applied to set the sample rate to the rate given
when creating an encoder or decoder
</doc>
  <field id="options::quiet_resampler_options::delay" name="delay">
    <brief>filter bank delay</brief>
    <type name="size_t" />
  </field>
  <field id="options::quiet_resampler_options::bandwidth" name="bandwidth">
    <brief>filter passband bandwidth</brief>
    <type builtin="yes" name="float" />
  </field>
  <field id="options::quiet_resampler_options::attenuation" name="attenuation">
    <brief>filter sidelobe suppression</brief>
    <type builtin="yes" name="float" />
  </field>
  <field id="options::quiet_resampler_options::filter_bank_size" name="filter_bank_size">
    <brief>filter bank size</brief>
    <type name="size_t" />
  </field>
</struct>
